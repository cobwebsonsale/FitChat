<h1>FitChat</h1>
<fieldset>
	<div class="blocktext">
		Hi <%= current_user.username %>, welcome to FitChat! </br>
		</br>
		Registered email: <%= current_user.email %> </br>
		<%= current_user.get_user_type %> since: <%= current_user.created_at %> </br>
		</br>
		<%= link_to "Chats", chat_inactive_index_path %> |
		<% if current_user.is_trainer %>
			<%= link_to "Requests", board_index_path %> |
		<% end %>
		<%= link_to("Logout", destroy_user_session_path, :method => :delete) %>
	</div>
</fieldset>
